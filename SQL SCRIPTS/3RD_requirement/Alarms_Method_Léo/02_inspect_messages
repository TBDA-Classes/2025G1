-- Step 2: Look at some alarm messages for chosen variables (e.g. 447, 557)

SELECT
  s.id_var,
  v.name AS variable_name,
  to_timestamp(s.date / 1000) AS log_time,   -- convert ms to timestamp
  LEFT(s.value, 200) AS message_preview
FROM variable_log_string s
JOIN variable v
  ON v.id = s.id_var
WHERE s.id_var IN (447)                      -- add others if needed: (447, 557, ...)
  AND s.value IS NOT NULL
  AND s.value <> '[]'
ORDER BY s.date
LIMIT 200;
